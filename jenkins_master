pipeline {
    agent any

    stages {
        
        stage('Welcome to master'){
            steps{
                echo 'Start Program'
            }
        }

        stage('Tigger the Slave Jobs') {
            steps {
                echo 'Wowwwww'
                build job: 'Slaves', parameters: [string(name: 'NAME', value: 'Boom'), string(name: 'Ages', value: '30')]
            }
        }
    }
}
